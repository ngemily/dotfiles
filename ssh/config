Host github
  User git
  Port 22
  Hostname github.com
  IdentityFile ~/.ssh-keys/github
  TCPKeepAlive yes
  IdentitiesOnly yes

Host ecf
  User ngemily2
  Hostname p170.ecf.toronto.edu
  IdentityFile ~/.ssh-keys/ecf
  TCPKeepAlive yes

Host bitbucket
  HostName bitbucket.org
  PreferredAuthentications publickey
  IdentityFile ~/.ssh-keys/bitbucket
  TCPKeepAlive yes

Host heroku.com
  Hostname heroku.com
  Port 22
  IdentitiesOnly yes
  IdentityFile ~/.ssh-keys/heroku
  TCPKeepAlive yes
  User ngemily
